package com.javarush.task.jdk13.task04.task0437;

/* 
Рисуем треугольник
*/

public class Solution {
    public static void main(String[] args) {
        for (int i = 0; i < 10; i++) 
        {
            for (int b = 0; b <= i; b++) 
            {
                System.out.print("8");
            }
            System.out.println();        
        }
    }
}
